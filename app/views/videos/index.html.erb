<h3>Videos</h3>
<div class="span8">
<table class="table table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Src</th>
    </tr>
  </thead>
  <tbody>
  <% @videos.each do |video| %>
    <tr>
      <td><%= link_to video.name, video %></td>
      <td><%= video.description %></td>
      <td><%= video.src %></td>
    </tr>
  <% end %>
  </tbody>
</table>
</div>